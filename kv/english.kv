#:include kv/english/english1.kv
#:include kv/english/english2.kv
#:include kv/english/english3.kv
#:include kv/english/english4.kv
#:include kv/english/english5.kv
#:include kv/english/english6.kv
#:include kv/english/english7.kv
#:include kv/english/english8.kv
#:include kv/english/english9.kv
#:include kv/english/english10.kv
<English>:
    BoxLayout:
        id : box
        name: 'box'
        MDAccordion:
            orientation: 'horizontal'
            size_hint_x: None
            width: '240dp'
            MDAccordionItem:
                title:'English'
                icon: 'book-open'
                MDAccordionSubItem:
                    text: "1"
                    on_release: sm.current = "one"
                MDAccordionSubItem:
                    text: "2"
                    on_release: sm.current = "two"
                MDAccordionSubItem:
                    text: "3"
                    on_release: sm.current = "three"
                MDAccordionSubItem:
                    text: "4"
                    on_release: sm.current = "four"
                MDAccordionSubItem:
                    text: "5"
                    on_release: sm.current = "five"
                MDAccordionSubItem:
                    text: "6"
                    on_release: sm.current = "six"
                MDAccordionSubItem:
                    text: "7"
                    on_release: sm.current = "seven"
                MDAccordionSubItem:
                    text: "8"
                    on_release: sm.current = "eight"
                MDAccordionSubItem:
                    text: "9"
                    on_release: sm.current = "nine"
                MDAccordionSubItem:
                    text: "10"
                    on_release: sm.current = "ten"
        ScreenManager:
            id: sm
            name : 'sm'
            Screen:
                id: welcome
                BoxLayout:
                    TextInput
                        text: 'ENGLISH'
                        disabled: True
            English1:
                id : one
                name : 'one'

            English2:
                id : two
                name : 'two'
            English3:
                id : three
                name : 'three'
            English4:
                id : four
                name : 'four'
            English5:
                id : five
                name : 'five'
            English6:
                id : six
                name : 'six'
            English7:
                id : seven
                name : 'seven'
            English8:
                id : eight
                name : 'eight'
            English9:
                id : nine
                name : 'nine'
            English10:
                id : ten
                name : 'ten'
<ToggleButton,Button>:
    padding: dp(10), dp(8)
    size_hint: None, None
    size: self.texture_size
<DemoLabel@Label>:
    canvas:
        Color:
            rgba: 216/255., 195/255., 88/255., 1
        Line:
            rectangle: self.x, self.y, self.width, self.height

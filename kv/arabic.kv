#:include kv/arabic/arabic1.kv
#:include kv/arabic/arabic2.kv
#:include kv/arabic/arabic3.kv
#:include kv/arabic/arabic4.kv
#:include kv/arabic/arabic5.kv
#:include kv/arabic/arabic6.kv
#:include kv/arabic/arabic7.kv
#:include kv/arabic/arabic8.kv
#:include kv/arabic/arabic9.kv
#:include kv/arabic/arabic10.kv
<Arabic>:
    BoxLayout:
        id : box
        name: 'box'
        MDAccordion:
            orientation: 'horizontal'
            size_hint_x: None
            width: '240dp'
            MDAccordionItem:
                title:'Arabic'
                icon: 'book-open'
                MDAccordionSubItem:
                    text: "1"
                    on_release: sm.current = "one"
                MDAccordionSubItem:
                    text: "2"
                    on_release: sm.current = "two"
                MDAccordionSubItem:
                    text: "3"
                    on_release: sm.current = "three"
                MDAccordionSubItem:
                    text: "4"
                    on_release: sm.current = "four"
                MDAccordionSubItem:
                    text: "5"
                    on_release: sm.current = "five"
                MDAccordionSubItem:
                    text: "6"
                    on_release: sm.current = "six"
                MDAccordionSubItem:
                    text: "7"
                    on_release: sm.current = "seven"
                MDAccordionSubItem:
                    text: "8"
                    on_release: sm.current = "eight"
                MDAccordionSubItem:
                    text: "9"
                    on_release: sm.current = "nine"
                MDAccordionSubItem:
                    text: "10"
                    on_release: sm.current = "ten"
        ScreenManager:
            id: sm
            name : 'sm'
            Screen:
                id: welcome
                BoxLayout:
                    TextInput
                        text: 'ARABIC'
                        disabled: True
            Arabic1:
                id : one
                name : 'one'

            Arabic2:
                id : two
                name : 'two'
            Arabic3:
                id : three
                name : 'three'
            Arabic4:
                id : four
                name : 'four'
            Arabic5:
                id : five
                name : 'five'
            Arabic6:
                id : six
                name : 'six'
            Arabic7:
                id : seven
                name : 'seven'
            Arabic8:
                id : eight
                name : 'eight'
            Arabic9:
                id : nine
                name : 'nine'
            Arabic10:
                id : ten
                name : 'ten'
<ToggleButton,Button>:
    padding: dp(10), dp(8)
    size_hint: None, None
    size: self.texture_size
<DemoLabel@Label>:
    canvas:
        Color:
            rgba: 216/255., 195/255., 88/255., 1
        Line:
            rectangle: self.x, self.y, self.width, self.height
